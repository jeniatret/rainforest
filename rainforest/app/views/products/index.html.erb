<h4><small><%= page_entries_info @products %></small></h4>
<h1>Products</h1> 

<%= link_to "Create new product", new_product_path %> 


<%= form_tag products_path, method: :get, authentication: false, id: 'search-form' do %>
	<%= text_field_tag :search, params[:search] %>
	<%= submit_tag "Search" %>
<% end %>

<div id="products">
	<%= render @products %>
</div>

<%= paginate @products %>
